{
  "name": "azure-devops-login",
  "description": "Uses the az tool to login provide Azure DevOps authentication in GitHub Actions",
  "version": "1.0.0",
  "author": "EchelonFour",
  "type": "module",
  "private": true,
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "test": "NODE_OPTIONS=--experimental-vm-modules NODE_NO_WARNINGS=1 jest",
    "lint": "eslint .",
    "lint:ci": "eslint -f=rdjson .",
    "local-action": "@github/local-action . src/main.ts .env",
    "package": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "format": "prettier --write .",
    "all": "pnpm run lint && pnpm run test && pnpm run coverage && pnpm run package"
  },
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@azure/identity": "^4.13.0",
    "fast-xml-parser": "^5.3.2",
    "ini": "^6.0.0"
  },
  "devDependencies": {
    "@github/local-action": "^6.0.2",
    "@jest/globals": "^30.2.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/ini": "^4.1.1",
    "@types/jest": "^30.0.0",
    "@types/mock-fs": "^4.13.4",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "eslint-config-intolerable-style-guide": "^5.0.8",
    "eslint-formatter-rdjson": "^1.0.6",
    "jest": "^30.2.0",
    "mock-fs": "^5.5.0",
    "rollup": "^4.53.2",
    "ts-jest": "^29.4.5",
    "ts-jest-resolver": "^2.0.1",
    "typescript": "^5.9.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "*"
  },
  "devEngines": {
    "runtime": {
      "name": "node",
      "onFail": "error"
    },
    "packageManager": {
      "name": "pnpm",
      "version": "~10.22.0",
      "onFail": "download"
    }
  },
  "engines": {
    "node": ">=24.10.0",
    "pnpm": ">=10.22.0"
  },
  "packageManager": "pnpm@10.22.0+sha512.bf049efe995b28f527fd2b41ae0474ce29186f7edcb3bf545087bd61fbbebb2bf75362d1307fda09c2d288e1e499787ac12d4fcb617a974718a6051f2eee741c"
}
